<h1>Listing registrations</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Mi</th>
    <th>Last name</th>
    <th>Suffix</th>
    <th>Class name</th>
    <th>Address 1</th>
    <th>Address 2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Province</th>
    <th>Country</th>
    <th>Email</th>
    <th>Gm street</th>
    <th>Gm colors</th>
    <th>Gm piaa</th>
    <th>Class colors</th>
    <th>Class motto</th>
    <th>Class flower</th>
    <th>Class size</th>
  </tr>

<% @registrations.each do |registration| %>
  <tr>
    <td><%=h registration.first_name %></td>
    <td><%=h registration.mi %></td>
    <td><%=h registration.last_name %></td>
    <td><%=h registration.suffix %></td>
    <td><%=h registration.class_name %></td>
    <td><%=h registration.address_1 %></td>
    <td><%=h registration.address_2 %></td>
    <td><%=h registration.city %></td>
    <td><%=h registration.state %></td>
    <td><%=h registration.zip %></td>
    <td><%=h registration.province %></td>
    <td><%=h registration.country %></td>
    <td><%=h registration.email %></td>
    <td><%=h registration.gm_street %></td>
    <td><%=h registration.gm_colors %></td>
    <td><%=h registration.gm_PIAA %></td>
    <td><%=h registration.class_colors %></td>
    <td><%=h registration.class_motto %></td>
    <td><%=h registration.class_flower %></td>
    <td><%=h registration.class_size %></td>
    <td><%= link_to 'Show', registration %></td>
    <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
    <td><%= link_to 'Destroy', registration, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New registration', new_registration_path %>